{
  "name": "leaflet-coverage",
  "version": "0.1.0",
  "main": "LayerFactory",
  "registry": "jspm",
  "format": "esm",
  "scripts": {
    "style": "standard",
    "test": "./node_modules/.bin/karma start --single-run",
    "karma": "./node_modules/.bin/karma start",
    "saucelabs": "./node_modules/.bin/karma start karma.conf-ci.js",
    "build": "jspm bundle-sfx leaflet-coverage/LayerFactory bundle.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "config.js",
      "karma.conf.js",
      "karma.conf-ci.js"
    ]
  },
  "jspm": {
    "directories": {
      "lib": "leaflet-coverage"
    },
    "dependencies": {
      "cwise-compiler": "npm:cwise-compiler@^1.1.2",
      "leaflet": "github:Leaflet/Leaflet@^0.7.3",
      "minified": "github:timjansen/minified.js@^2015.1.1",
      "ndarray": "npm:ndarray@^1.0.18"
    },
    "devDependencies": {
      "assert": "npm:assert@^1.3.0",
      "babel": "npm:babel-core@^5.8.22",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.1.0"
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "jspm": "^0.16.1",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "karma-sauce-launcher": "^0.2.14",
    "mocha": "^2.2.5",
    "standard": "^5.1.0"
  }
}
