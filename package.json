{
  "name": "leaflet-coverage",
  "version": "0.1.0",
  "description": "A Leaflet plugin for visualizing coverage data",
  "repository": {
    "type": "git",
    "url": "https://github.com/Reading-eScience-Centre/leaflet-coverage.git"
  },
  "bugs": {
    "url": "https://github.com/Reading-eScience-Centre/leaflet-coverage/issues"
  },
  "author": {
    "name": "Maik Riechert"
  },
  "files": [
    "leaflet-coverage.src.js",
    "leaflet-coverage.min.js"
  ],
  "registry": "jspm",
  "format": "esm",
  "scripts": {
    "docs": "esdoc -c esdoc.json",
    "postinstall": "jspm install",
    "style": "standard",
    "test": "karma start --single-run",
    "karma": "karma start",
    "saucelabs": "karma start karma.conf-ci.js",
    "build": "npm run build-src && npm run build-min",
    "build-src": "jspm bundle-sfx leaflet-coverage/classic - leaflet leaflet-coverage.src.js --globals \"{leaflet:'L'}\" --format global --skip-source-maps",
    "build-min": "jspm bundle-sfx leaflet-coverage/classic - leaflet leaflet-coverage.min.js --globals \"{leaflet:'L'}\" --format global --minify --skip-source-maps",
    "prepublish": "npm run build"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "config.js",
      "karma.conf.js",
      "karma.conf-ci.js"
    ]
  },
  "jspm": {
    "directories": {
      "lib": "leaflet-coverage"
    },
    "files": null,
    "main": "LayerFactory",
    "dependencies": {
      "cwise-compiler": "npm:cwise-compiler@^1.1.2",
      "leaflet": "github:Leaflet/Leaflet@^0.7.3",
      "minified": "github:timjansen/minified.js@^2015.1.1",
      "ndarray": "npm:ndarray@^1.0.18"
    },
    "devDependencies": {
      "assert": "npm:assert@^1.3.0",
      "babel": "npm:babel-core@^5.8.22",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.1.0"
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "esdoc": "^0.3.0",
    "esdoc-importpath-plugin": "0.0.1",
    "jspm": "^0.16.2",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "karma-sauce-launcher": "^0.2.14",
    "mocha": "^2.2.5",
    "standard": "^5.1.0"
  }
}
