{
  "name": "leaflet-coverage",
  "version": "0.1.0",
  "main": "CoverageLayerFactory",
  "registry": "jspm",
  "format": "esm",
  "scripts": {
    "style": "standard",
    "test": "mocha test/",
    "build": "jspm bundle-sfx src/CoverageLayerFactory bundle.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "config.js"
    ]
  },
  "jspm": {
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "cwise-compiler": "npm:cwise-compiler@^1.1.2",
      "leaflet": "github:Leaflet/Leaflet@^0.7.3",
      "ndarray": "npm:ndarray@^1.0.18"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.22",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.1.0"
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "jspm": "^0.16.1",
    "mocha": "^2.2.5",
    "standard": "^5.1.0"
  }
}
